@page
@model LoginModel

<style data-tag="reset-style-sheet">
      html {  line-height: 1.15;}body {  margin: 0;}* {  box-sizing: border-box;  border-width: 0;  border-style: solid;}p,li,ul,pre,div,h1,h2,h3,h4,h5,h6,figure,blockquote,figcaption {  margin: 0;  padding: 0;}button {  background-color: transparent;}button,input,optgroup,select,textarea {  font-family: inherit;  font-size: 100%;  line-height: 1.15;  margin: 0;}button,select {  text-transform: none;}button,[type="button"],[type="reset"],[type="submit"] {  -webkit-appearance: button;}button::-moz-focus-inner,[type="button"]::-moz-focus-inner,[type="reset"]::-moz-focus-inner,[type="submit"]::-moz-focus-inner {  border-style: none;  padding: 0;}button:-moz-focus,[type="button"]:-moz-focus,[type="reset"]:-moz-focus,[type="submit"]:-moz-focus {  outline: 1px dotted ButtonText;}a {  color: inherit;  text-decoration: inherit;}input {  padding: 2px 4px;}img {  display: block;}html { scroll-behavior: smooth  }
    </style>
    <style data-tag="default-style-sheet">
      html {
        font-family: Poppins;
        font-size: 16px;
      }

      body {
        font-weight: 400;
        font-style:normal;
        text-decoration: none;
        text-transform: none;
        letter-spacing: normal;
        line-height: 1.15;
        color: var(--dl-color-scheme-green100);
        background-color: var(--dl-color-scheme-yellow20);

      }
    </style>
    <link
      rel="shortcut icon"
      href="~/img/logo1.png"
      type="icon/png"
      sizes="32x32"
    />
    <link
      rel="stylesheet"
      href="https://fonts.googleapis.com/css2?family=Lexend:wght@100;200;300;400;500;600;700;800;900&amp;display=swap"
      data-tag="font"
    />
    <link
      rel="stylesheet"
      href="https://fonts.googleapis.com/css2?family=Poppins:ital,wght@0,100;0,200;0,300;0,400;0,500;0,600;0,700;0,800;0,900;1,100;1,200;1,300;1,400;1,500;1,600;1,700;1,800;1,900&amp;display=swap"
      data-tag="font"
    />
    <link
      rel="stylesheet"
      href="https://fonts.googleapis.com/css2?family=Urbanist:ital,wght@0,100;0,200;0,300;0,400;0,500;0,600;0,700;0,800;0,900;1,100;1,200;1,300;1,400;1,500;1,600;1,700;1,800;1,900&amp;display=swap"
      data-tag="font"
    />
    <link
      rel="stylesheet"
      href="https://fonts.googleapis.com/css2?family=Titillium+Web:ital,wght@0,200;0,300;0,400;0,600;0,700;0,900;1,200;1,300;1,400;1,600;1,700&amp;display=swap"
      data-tag="font"
    />
    <!--This is the head section-->
    <!-- <script type="text/javascript"> ... </script> -->
    <link rel="stylesheet" href="~/css/styles.css" />
    <link href="~/css/login.css" rel="stylesheet" />

<div>


    <div class="log-in-page-container">
        <div class="navbar-container">
            <a class="log-in-page-logo" asp-controller="Home" asp-action="Index">
                <img alt="image"
                     src="~/img/logo-200h.png"
                     class="log-in-page-image" />
                <span class="brandName">
                    <span class="log-in-page-text01">NMIS-</span>
                    <span>&nbsp;SMFA</span>
                </span>
            </a>
            <div class="log-in-page-links">
                <a class="log-in-page-home" asp-controller="Home" asp-action="Index">HOME</a>
                <a class="log-in-page-articles" href="/Home/Index#articles">ARTICLES</a>
                <a class="log-in-page-faqs" href="/Home/Index#FAQs">FAQS</a>
                <a class="log-in-page-aboutus" href="/Home/Index#footer">ABOUT US</a>
                <a class="button-secondary button" asp-controller="Trace" asp-action="Index">TRACE</a>
                <a asp-area="Identity" asp-page="/Account/Login">
                    <button class="button button-primary">LOG IN</button>
                </a>
            </div>
            <button class="log-in-page-burger-menu navbar-burger-menu">
                <a data-toggle="dropdown" aria-haspopup="true" aria-expanded="false">
                    <svg viewBox="0 0 1024 1024" class="log-in-page-icon">
                        <path d="M128 256h768v86h-768v-86zM128 554v-84h768v84h-768zM128 768v-86h768v86h-768z"></path>
                    </svg>
                    <div class="dropdown-menu dropdown-menu-right">
                        <a class="dropdown-item" asp-controller="Home" asp-action="Index">HOME</a>
                        <a class="dropdown-item" href="/Home/Index#articles">ARTICLES</a>
                        <a class="dropdown-item" href="/Home/Index#FAQs">FAQS</a>
                        <a class="dropdown-item" href="/Home/Index#footer">About Us</a>
                        <div class="dropdown-divider"></div>
                        <a class="dropdown-item" asp-controller="Trace" asp-action="Index">TRACE</a>
                        <a class="dropdown-item" asp-area="Identity" asp-page="/Account/Login">LOG IN</a>
                    </div>
                </a>
            </button>
        </div>
       
            <div class="log-in-page-container01">
            <div class="log-in-page-container02">
                <img alt="image"
                     src="~/img/Login Page/login-background1.svg"
                     class="log-in-page-image1" />
                <div class="log-in-page-container03">
                    <div class="log-in-page-container04">
                        <a asp-controller="Home" asp-action="Index" class="log-in-page-navlink">
                            <svg viewBox="0 0 1024 1024" class="log-in-page-icon2">
                                <path d="M854 470v84h-520l238 240-60 60-342-342 342-342 60 60-238 240h520z"></path>
                            </svg>
                        </a>
                        <div class="log-in-page-container05">
                            <img alt="image"
                                 src="~/img/logo-200h.png"
                                 class="log-in-page-image2" />
                            <h1 class="log-in-page-text03">
                                <span class="log-in-page-text04">NMIS</span>
                                <span>- SMFA</span>
                            </h1>
                            <div class="log-in-page-container06">
                                <a asp-area="Identity" asp-page="/Account/Register"
                                   class="log-in-page-registerclick">
                                    Register Now!
                                </a>
                                <span class="log-in-page-text06">(MTV Application)</span>
                            </div>
                        </div>
                    </div>
                    <form id="account" method="post">
                        
                        <span class="log-in-page-text07">Sign in your account</span>
                        <div class="log-in-page-container07">
                            <span class="log-in-page-text08">Email</span>
                            <input asp-for="Input.Email" class="log-in-page-email input" autocomplete="username" aria-required="true" />
                            <span asp-validation-for="Input.Email" class="text-danger"></span>

                        </div>
                        <div class="log-in-page-container08">
                            <span class="log-in-page-text09">Password</span>
                            <input asp-for="Input.Password" class="log-in-page-email1 input" autocomplete="current-password" aria-required="true" />
                            <span asp-validation-for="Input.Password" class="text-danger"></span>
                            <div asp-validation-summary="All" class="text-danger"></div>
                        </div>
                        <div class="log-in-page-container09">
                            <span class="log-in-page-text10">Forgot your password?</span>
                        </div>
                        <div class="log-in-page-container10">
                            <input type="checkbox" asp-for="Input.RememberMe" class="mx-2"/>
                            @Html.DisplayNameFor(m => m.Input.RememberMe)
                        </div>
                        <button id="login-submit" type="submit" class="log-in-page-signinbtn button button-transparent">Log in</button>
                        <div class="log-in-page-container11">
                            <button type="button" class="log-in-page-googlebtn button">
                                <svg viewBox="0 0 1316.5714285714284 1024"
                                     class="log-in-page-icon4">
                                    <path d="M821.143 521.714c0 239.429-160.571 409.143-402.286 409.143-231.429 0-418.857-187.429-418.857-418.857s187.429-418.857 418.857-418.857c113.143 0 207.429 41.143 280.571 109.714l-113.714 109.143c-30.857-29.714-85.143-64.571-166.857-64.571-142.857 0-259.429 118.286-259.429 264.571s116.571 264.571 259.429 264.571c165.714 0 228-119.429 237.714-180.571h-237.714v-144h395.429c4 21.143 6.857 42.286 6.857 69.714zM1316.571 452v120h-119.429v119.429h-120v-119.429h-119.429v-120h119.429v-119.429h120v119.429h119.429z"></path>
                                </svg>
                                <span class="log-in-page-text13">Sign in with Google</span>
                            </button>
                        </div>
                    </form>
                </div>
            </div>
        </div>
        

    </div>
</div>






@section Scripts {
    <partial name="_ValidationScriptsPartial" />
}
